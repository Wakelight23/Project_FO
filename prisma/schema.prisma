generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TeamMebertest {
  teamMemberId Int      @id @default(autoincrement()) @map("teamMemberId")
  playerId     Int      @map("playerId")
  upgrade      Int      @default(0) @map("upgrade")
  isSelected   Boolean  @default(false) @map("isSelected")
  createdAt    DateTime @default(now()) @map("createdAt")
  updatedAt    DateTime @default(now()) @updatedAt @map("updatedAt")

  @@map("teamtest")
}

model Playertest {
  playerId   Int      @id @default(autoincrement()) @map("playerId")
  playerName String   @map("playerName")
  playerStat Int      @map("playerStat")
  createdAt  DateTime @default(now()) @map("createdAt")
  updatedAt  DateTime @default(now()) @updatedAt @map("updatedAt")

  @@map("playertest")
}
